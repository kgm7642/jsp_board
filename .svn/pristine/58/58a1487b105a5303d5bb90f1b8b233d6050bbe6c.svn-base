package inherit;

import runningmachine.Runner;

public class Dog extends Animal implements Runner{

	// 품종
	private String kind;
	
	// 뛰다
	public void run() {
		System.out.println("dog run");
	}
	
	// 짖다
	public void bark() {
		System.out.println("bark");
	}

	public String getKind() {
		return kind;
	}

	public void setKind(String kind) {
		this.kind = kind;
	}

	
	
}
